{
    "manifest_version": 2,
    "name": "Route zezo.org",
    "description": "Extract route for MultiRaces",
    "version": "3.1",
    "page_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "permissions": ["tabs","storage"],
    "content_scripts": [
        {
            "matches": ["http://*.zezo.org/*/chart.pl*"],
            "js": ["myscript.js"]
        }
    ],
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "background": {
        "scripts": ["eventPage.js"]
    }
}